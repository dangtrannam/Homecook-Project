<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="dtos.DishInDTO">
        <table name="DishIn" schema="dbo" catalog="homecook"/>
        <id-class class="dtos.DishInDTOPK"/>
        <attributes>
            <id name="menuId">
                <column name="MenuID" precision="-1"/>
            </id>
            <id name="dishId">
                <column name="DishID" precision="-1"/>
            </id>
            <many-to-one name="dishesByDishId" optional="true" target-entity="dtos.DishesDTO">
                <join-column name="DishID" referenced-column-name="DishID"/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
